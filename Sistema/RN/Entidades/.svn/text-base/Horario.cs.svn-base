using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace RN.Entidades
{
    public class Horario
    {
        #region Atributos
        private int id;
        private string horaInicio;
        private string horaFin;
        private int salaId;
        private bool finDeSemana;
        private bool estado;
        #endregion
        #region Propiedades
        public int Id
        {
            get { return id; }
            set { id = value; }
        }
        public string HoraInicio
        {
            get { return horaInicio; }
            set { horaInicio = value; }
        }
        public string HoraFin
        {
            get { return horaFin; }
            set { horaFin = value; }
        }
        public int SalaId
        {
            get { return salaId; }
            set { salaId = value; }
        }
        public bool FinDeSemana
        {
            get { return finDeSemana; }
            set { finDeSemana = value; }
        }
        public bool Estado
        {
            get { return estado; }
            set { estado = value; }
        }
       
        #endregion
        #region Metodos
        public Horario()
        {
            id = -1;
            salaId = 0;
            horaInicio = string.Empty;
            horaFin = string.Empty;
            finDeSemana = false;
            estado = true;
        }
        #endregion
    }
}
